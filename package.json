{
  "name": "adventure",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "4.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.10",
    "remote-redux-devtools": "^0.5.12"
  },
  "scripts": {
    "build-css": "node-sass src/resources/scss -o src/resources/css",
    "watch-css": "npm run build-css && node-sass src/resources/scss -o src/resources/css --watch --recursive --output-style compressed",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && node scripts/build.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
